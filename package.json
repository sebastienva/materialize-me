{
  "name": "materialize-me",
  "version": "0.0.1",
  "description": "coming",
  "scripts": {
    "start": "cd demo && npm start",
    "build": "webpack",
    "dev": "cd demo && npm run dev",
    "lint": "eslint src",
    "test": "mocha test/helpers/browser.js src/**/**.js",
    "cover": "cross-env NODE_ENV=test nyc --reporter=html npm test",
    "coveralls": "cross-env NODE_ENV=test nyc --reporter=text-lcov npm test | coveralls",
    "doc": "./node_modules/.bin/react-docgen src/ | ./doc/buildDocs.sh",
    "postinstall": "cd demo && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastienva/materialize-me.git"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "react",
    "reactjs",
    "material"
  ],
  "author": "sv",
  "license": "MIT",
  "homepage": "https://github.com/sebastienva/materialize-me.git",
  "devDependencies": {
